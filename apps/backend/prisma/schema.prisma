datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

generator typegraphql {
  provider = "typegraphql-prisma"
}

model Note {
  id        Int      @id @default(autoincrement())
  title     String
  body      String
  updatedAt DateTime @updatedAt
}
